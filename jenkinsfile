pipeline {
    agent any

    stages {
        
                stage('git') {
            steps {
            
                git branch: 'marwen', url: 'https://github.com/Mohamedazizgh/DevOpsSpring.git',
                credentialsId:"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";
                
            }
}
        
       stage('MVN Package'){
            steps {
                sh """mvn -version  """
                sh """java -version """
               sh """mvn package -e """
            }
        }
        
      stage("MVN Compile"){
            steps {
                sh """mvn compile -e """
                
            }
        }
           stage("MVN Install"){
            steps {
                sh """mvn install """
                
            }
        }
        stage("MVN Clean"){
            steps {
                sh """mvn clean -e """
                
            }
        }
        
        
    
       
            
        
  
            
        
                
            

    }
}
